{
    "name": "loumray/fastimagesize",
    "type": "library",
    "description": "This package provides a getimagesize function that aims to match the usage of PHP getimagesize while trying to avoid the performance cost of downloading the complete file.",
    "keywords": ["getimagesize", "fast", "PHP", "image", "size", "imagesize"],
    "homepage": "https://github.com/loumray/fastimagesize",
    "license": "MIT",
    "authors": [
        {
            "name": "Marc Alexander",
            "email": "admin@m-a-styles.de",
            "homepage": "https://www.m-a-styles.de",
            "role": "Developer"
        },
        {
          "name": "Louis-Michel Raynauld",
          "homepage": "https://www.onecodeshop.com",
          "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8|^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "FastImageSize\\": "src/",
            "FastImageSize\\tests\\": "tests/"
        },
        "files": ["src/functions.php"]
    }
}
